extends core/_template

append sharedVars
    - var activeMenu = 'Поиск';

block title
    title Поиск

block content
    //workarea
    .workarea
        .container
            .breadcrumbs
                .breadcrumbs__item
                    a.breadcrumbs__link.breadcrumbs__link_home(href="/")
                .breadcrumbs__item
                    a.breadcrumbs__link(href="/") каталог
                .breadcrumbs__current
                    span поиск по каталогу
            //end .breadcrumbs
        .container-h
            .inner-page
                .subcategory
                    .subcategory__search-ttl
                        .subcategory__search-h
                            h1 Результат поиска по запросу «тарелка»
                        .subcategory__search-find Нашлось 78 товаров
                    .subcategory__search-head
                        .subcategory__search
                            form.search
                                .search__input
                                    input.input.input_gray(type="text")
                                .search__btn
                                    button.btn.btn_m.btn_80.btn-red(type="submit") Искать снова
                        .subcategory__search-quant
                            a.subcategory__quant-i.active(href="#") 15
                            a.subcategory__quant-i(href="#") 30
                            a.subcategory__quant-i(href="#") 50
                            a.subcategory__quant-i(href="#") 100
                        .subcategory__view
                            a.subcategory__view-i.subcategory__view-grid.active(href="#")
                            a.subcategory__view-i.subcategory__view-list(href="#")
                    //end .subcategory__head
                    .subcategory__body
                        .subcategory__item
                            .subcategory__i-img(style="background-image:url('images/TEMP/c1.png')")
                            .subcategory__i-body
                                .subcategory__i-inf
                                    .subcategory__i-ttl Одноразовая тарелка 7’’
                                    .subcategory__i-art 347RP10WHSS
                                .subcategory__i-price
                                    span.price.subcategory__int 3.
                                    span.price.subcategory__kop 96
                                    span.rub.subcategory__rub И
                                    span.sep.subcategory__sep /
                                    span.st.subcategory__st шт
                            .subcategory__i-footer
                                form.to-basket
                                    .to-basket__input
                                        input.value(type="number" min="1" value="10")
                                    .to-basket__btn
                                        button.btn.btn_s.btn-green(type="submit") В корзину
                        //end .subcategory__item
                        .subcategory__item
                            .subcategory__i-img(style="background-image:url('images/TEMP/c2.png')")
